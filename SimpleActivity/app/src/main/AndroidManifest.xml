<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.visualstudio.simpleactivity">

    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LearnActivity.MainActivity" />
        <activity
            android:name=".LearnActivity.DialogActivity"
            android:theme="@android:style/Theme.Dialog" />
        <activity android:name=".LearnContentProvider.ContentProviderActivity" />
        <activity android:name=".Home.HomeActivity" />
        <activity android:name=".Login.LoginActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LearnActivity.IntentImplicitItemActivity" />
        <activity android:name=".LearnActivity.IntentForResultActivity" />
        <activity android:name=".LearnActivity.IntentForResultEditNameActivity" />
        <activity android:name=".LearnActivity.IntentCameraActivity"></activity>
    </application>

</manifest>