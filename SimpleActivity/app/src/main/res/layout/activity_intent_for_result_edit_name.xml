<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center"
    tools:context="com.example.visualstudio.simpleactivity.LearnActivity.IntentForResultEditNameActivity">

    <EditText
        android:id="@+id/activity_intent_for_result_edit_name_edt_userName"
        android:layout_width="200dp"
        android:layout_height="wrap_content"
        android:hint="Nhập tên cần sửa"
        android:layout_marginTop="10dp"
        />
    <com.gc.materialdesign.views.ButtonRectangle
        android:id="@+id/activity_intent_for_result_edit_name_btn_saveName"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:text="Save Name"
        android:layout_marginTop="10dp"
        android:onClick="onClickBtnIntentExplicit"/>

</LinearLayout>
